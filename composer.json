{
  "name": "serakoi/flarum-discord-theme",
  "description": "Add a discord theme to your forum!",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0.0"
  },
  "authors": [
    {
      "name": "Seer",
      "email": "admin@seer-software.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Serakoi\\FlarumDiscordTheme\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Discord Theme",
      "icon": {
        "name": "fab fa-discord",
        "backgroundColor": "#7289DA",
        "color": "#FFFFFF"
      }
    },
    "flagrow": {
      "discuss": "https://discuss.flarum.org/d/27648-discord-theme"
    }
  }
}
